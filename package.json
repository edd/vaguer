{
  "name": "@vegaprotocol/vaguer",
  "version": "2.0.1",
  "description": "Basic diagnostic tool for comparing vega data nodes",
  "type": "module",
  "bin": "src/bin.js",
  "engines": {
    "node": ">=0.17.5"
  },
  "scripts": {
    "start": "node --no-warnings --experimental-fetch src/bin.js",
    "test": "npx tape src/**/*.test.js | tap-spot",
    "lint": "npx standard",
    "lint:fix": "npx standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vegaprotocol/vaguer.git"
  },
  "author": "Vega <hi@vega.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vegaprotocol/vaguer/issues"
  },
  "homepage": "https://github.com/vegaprotocol/vaguer#readme",
  "precommit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "console-table-printer": "^2.11.0",
    "diff": "^5.1.0",
    "fast-json-stable-stringify": "^2.1.0",
    "fast-sha256": "^1.3.0",
    "lodash.countby": "^4.6.0",
    "lodash.head": "^4.0.1",
    "lodash.last": "^3.0.0",
    "lodash.maxby": "^4.6.0",
    "lodash.pairs": "^3.0.1",
    "lodash.sample": "^4.2.1",
    "lodash.sortby": "^4.7.0",
    "minimist": "^1.2.8",
    "standard": "^17.0.0",
    "tap-spot": "^1.1.2",
    "tape": "^5.6.3",
    "toml": "^3.0.0",
    "valid-url": "^1.0.9"
  }
}
